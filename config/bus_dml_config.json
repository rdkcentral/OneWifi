		"uint16_t": {
			"type": "integer",
			"minimum": 0,
			"maximum": 65535
		},
		"uint8_t": {
			"type": "integer",
			"minimum": 0,
			"maximum": 255
		},
		"int32_t": {
			"type": "integer",
			"maximum": 2147483647,
			"minimum": -2147483648
		},
		"ap_retry_limit_t": {
			"title": "ap retry limit",
			"type": "integer",
			"maximum": 255,
			"minimum": 0
		},
		"bandwidth_t": {
			"title": "Radio Bandwidth",
