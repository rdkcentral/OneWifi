			"title": "WHIX Log Interval",
			"type": "integer",
			"maximum": 3600,
			"minimum": 300
		},
		"wifi_status_t": {
			"title": "WiFi radio/Vap status information",
			"type": "string",
			"enum": [
				"Up",
				"Down",
				"Unknown",
				"Dormant",
				"NotPresent",
				"LowerLayerDown",
				"Error",
				"DeepSleep"
