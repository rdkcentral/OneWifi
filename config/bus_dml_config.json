		"encryption_method_t": {
			"title": "encryption method",
			"type": "string",
			"enum": [
				"TKIP",
				"AES",
				"AES+TKIP"
			]
		},
		"frag_threshold_t": {
			"title": "frag threshold",
			"type": "integer",
			"maximum": 2346,
			"minimum": 256
		},
		"guard_interval_t": {
			"title": "guard interval",
			"type": "string",
			"enum": [
				"400nsec",
				"800nsec",
				"Auto",
				"1600nsec",
				"3200nsec"
			]
		},
		"macaddr_t": {
			"title": "MAC Address",
			"type": "string",
			"maxLength": 17,
			"minLength": 17,
			"pattern": "^([0-9a-fA-F][0-9a-fA-F]:){5}[0-9a-fA-F][0-9a-fA-F]$"
		},
		"rapid_reconnect_max_time_t": {
			"title": "rapid reconnect max time",
			"type": "integer",
			"maximum": 1200,
			"minimum": 15
